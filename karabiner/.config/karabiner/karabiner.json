      "complex_modifications": {
        "parameters": { "basic.to_if_held_down_threshold_milliseconds": 200 },
        "rules": [
          {
            "description": "Caps Lock + Shift -> Caps Lock",
            "manipulators": [
              {
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "mandatory": ["shift"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [{ "key_code": "caps_lock" }],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Caps Lock -> LeftCommand+Control+Shift+Option, Caps Lock -> Escape (if held alone for > 500 ms)",
            "manipulators": [
              {
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": { "optional": ["any"] }
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": ["left_option", "left_command", "left_control"]
                  }
                ],
                "to_if_alone": [
                  {
                    "hold_down_milliseconds": 500,
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Add grave accent to non_us_backslash key",
            "manipulators": [
              {
                "from": {
                  "key_code": "non_us_backslash",
                  "modifiers": {
                    "mandatory": ["right_option"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "grave_accent_and_tilde",
                    "modifiers": []
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "non_us_backslash",
                  "modifiers": {
                    "mandatory": ["left_option"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "grave_accent_and_tilde",
                    "modifiers": []
                  }
                ],
                "type": "basic"
              }
            ]
          },
